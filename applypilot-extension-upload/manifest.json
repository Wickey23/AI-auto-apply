{
  "manifest_version": 3,
  "name": "ApplyPilot - Job Clipper",
  "version": "1.0",
  "description": "Clip job postings to your local ApplyPilot dashboard.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "http://localhost:3000/*"
  ],
  "action": {
    "default_popup": "popup.html"
  }
}